<Page x:Class="ShopView.View.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShopView.View"
      xmlns:uc="clr-namespace:ShopView.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page1">

    <Grid>
        <uc:ClientUserControl/>
        <StackPanel>
            <Button x:Name="navigationBack" Content="navigate back" Click="navigation_Click" />
            <Button x:Name="navigationThrow" Content="navigate throw" Click="navigationThrow_Click"/>
            <Button x:Name="navigationframe" Content="navigate frame" Click="navigationframe_Click" />
        </StackPanel>
    </Grid>
</Page>
